{
    "name": "t-sui",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "check": "tsc --noEmit",
        "predev": "npm run check",
        "dev": "node --import tsx examples/main.ts",
        "dev:watch": "node --import tsx --watch examples/main.ts",
        "dev:bun": "bun examples/main.ts",
        "test": "node --import tsx --test --test-force-exit examples/**/*.test.ts",
        "test:file": "node --import tsx --test",
        "test:dev": "node --import tsx",
        "postinstall": "playwright install chromium || true"
    },
    "devDependencies": {
        "@playwright/test": "^1.57.0",
        "@types/node": "^22.19.3",
        "playwright": "^1.57.0",
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "tsx": "^4.21.0"
    }
}